{
    "version": 2,
    "builds": [
        {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
        "src": "public/**",
        "use": "@vercel/static"

    }
],
"rewrites": [
    {
      "source": "/cars",
      "destination": "public/dist"
    }
  ],
    "routes": [
      {
        "src": "/redirect",
        "status": 308,
        "headers": { "Location": "https://google.com/" }
      },
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/",
        "dest": "public/index.html"
      },
       {
        "src": "/(.*)",
        "dest": "public/$1"
      }
    ]
  }