{
    "version": 2,
    "builds": [
        {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
        "src": "public/**",
        "use": "@vercel/static"

    }
],
    "routes": [
      {
        "src": "/redirect",
        "status": 308,
        "headers": { "Location": "https://google.com/" }
      },
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      
      {
        "src": "/",
        "dest": "public/index.html"
      },
      {
        "src": "/cars",
        "headers": { "cache-control": "s-maxage=1000" },
        "dest": "public/dist/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "public/$1"
      }
    ]
  }