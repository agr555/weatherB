{
    "version": 2,
    "builds": [
        {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
        "src": "public/**",
        "use": "@vercel/static"

    }
],
    "routes": [
      {
        "src": "/redirect",
        "status": 308,
        "headers": { "Location": "https://google.com/" }
      },
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      
    
      {
        "src": "/cars",
        "headers": { "cache-control": "s-maxage=1000" },
        "dest": "public/dist/index.html"
      },
      {
        "src": "/car",
        "headers": { "cache-control": "s-maxage=1000" },
        "dest": "/public/dist/index.html"
      },
      {
        "src": "/weather",
        "headers": { "cache-control": "s-maxage=1000" },
        "dest": "public/weather/index.html"
      },
 
       {
        "src": "/(.)",
        "dest": "public/$1"
      },
      {
        "src": "/(.cars)",
        "dest": "public/dist/$1"
      }

    ]
  }