{
    "version": 2,
    "builds": [
        {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
        "src": "public/**",
        "use": "@vercel/static"

    }
],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      
      {
        "src": "/",
        "dest": "public/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "public/$1"
      }
    ]
  }


  {
    "src": "/(.+)",
    "dest": "public/$1"
  }

  {
    "src": "/",
    "dest": "public/index.html"
  }

  ======
  //одинаковык пути для выбора каталога
  {
    "src": "/",
    "dest": "public/index.html"
  },
  {
    "src": "/(.*)",
    "dest": "public/dist/$1"
  }

//кеш
  {
    "src": "/weather",
    "headers": { "cache-control": "s-maxage=1000" },
    "dest": "public/weather/$1"
  },